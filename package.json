{
  "name": "barbershop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:prod": "set NODE_ENV=production && next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
